    "inquirer": "^8.2.6",
    "cli-table3": "^0.6.3",
    "ora": "^5.4.1",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "node-fetch": "^2.7.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1"
