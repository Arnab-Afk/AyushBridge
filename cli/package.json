{
  "name": "ayushbridge-cli",
  "version": "1.0.0",
  "description": "Command Line Interface for AyushBridge FHIR Backend",
  "main": "index.js",
  "bin": {
    "ayush": "./bin/ayush.js",
    "ayushbridge": "./bin/ayush.js"
  },
  "scripts": {
